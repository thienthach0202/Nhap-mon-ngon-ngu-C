#include <stdio.h>

int main(void) {
  printf("bang cuu chuong\n");
  int a,b;
  printf("\nbang nhan2\n");
  for(a=2,b=0;b<10;b++){
     printf("\t\n%d * %d = %d\n",a,b,a*b);
    }
    printf("\nbang nhan3\n");
  for(a=3,b=0;b<10;b++){
     printf("\t\n%d * %d = %d\n",a,b,a*b);
    }
    printf("\nbang nhan4\n");
  for(a=4,b=0;b<10;b++){
     printf("\t\n%d * %d = %d\n",a,b,a*b);
    }
  return 0;
}
